#!/bin/sh

# This script is used for showing programs with desktop entries

# Defaults
lines="$DMENU_LINES"
border="$DMENU_BORDER"
height="$DMENU_HEIGHT"
prompt="ó°€» Applications:"

# dmenu command
DMENU="dmenu -l $lines -bw $border -h $height -p '$prompt' -c -n"

# Command
j4-dmenu-desktop --dmenu="$DMENU" --term="$TERMINAL"
